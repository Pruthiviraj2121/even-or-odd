package com.company;

import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enetr a number: ");
        int x = sc.nextInt();
        String eo = (x % 2 ==0)?"even":"odd";
        System.out.println(x + "is" + eo);

    }
}
